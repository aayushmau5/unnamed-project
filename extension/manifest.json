{
	"manifest_version": 3,
	"name": "phx.aayushsahu.com",
	"description": "web extension connected to phoenix.aayushsahu.com",
	"version": "1.0",
	"permissions": [
		"tabs",
		"alarms",
		"webRequest"
	],
	"background": {
		"service_worker": "scripts/background.js",
		"scripts": [
			"scripts/phoenix.min.js",
			"scripts/background.js"
		]
	},
	"action": {
		"default_popup": "main.html",
		"default_icon": {
			"16": "icons/favicon-16x16.png",
			"32": "icons/favicon-32x32.png"
		}
	},
	"icons": {
		"16": "icons/favicon-16x16.png",
		"32": "icons/favicon-32x32.png"
	}
}